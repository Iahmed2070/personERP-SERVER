<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>PePlatFrom Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <!-- Ajax Check UserLogin is Exsits? -->
    <service name="isUserLoginExsits" engine="java"
             location="main.java.com.banfftech.personerp.peplatform.PersonEventLoginWorker" invoke="isUserLoginExsits" auth="false">
        <description>Ajax Check UserLogin is Exsits?</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>


    <!-- Get Captcha To Login -->
    <service name="getLoginCaptcha" engine="java"
             location="main.java.com.banfftech.personerp.peplatform.PlatformServices" invoke="getLoginCaptcha" auth="false">
        <description>Get Captcha To Login</description>
        <attribute name="teleNumber" type="String" mode="IN" optional="false"/>
        <attribute name="smsType" type="String" mode="IN" optional="false"/>
    </service>
    

</services>