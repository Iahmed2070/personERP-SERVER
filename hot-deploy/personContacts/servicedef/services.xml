<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>PersonErp Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service>
    <service name="findPersonInfo" engine="java"
            location="com.banfftech.personerp.PersonErpQueryService" invoke="findPersonInfo" auth="false">
        <description>查询用户信息</description>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    
    <service name="findContactPerson" engine="java"
            location="com.banfftech.personerp.PersonErpQueryService" invoke="findContactPerson" auth="false">
        <description>查询联系人个人信息</description>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    
    <service name="showPersonAddress" engine="java" location="com.banfftech.personerp.PersonErpQueryService" invoke="showPersonAddress" auth="false">
    	<description>查询用户地址(地址修改页面)</description>
    	<attribute name="partyId" mode="IN" type="String" optional="false"/>
    	<attribute name="resultMap" type="java.util.HashMap" mode="OUT" optional="true"/>
    </service>
    
    <service name="editPersonAddress" engine="java" location="com.banfftech.personerp.PersonErpQueryService" invoke="editPersonAddress" auth="false">
    	<description>编辑用户地址</description>
    	<attribute name="stateProvinceGeoId" mode="IN" type="String" optional="false"/>
    	<attribute name="geoIdCity" mode="IN" type="String" optional="false"/>
    	<attribute name="geoIdArea" mode="IN" type="String" optional="false"/>
    	<attribute name="address1" mode="IN" type="String" optional="false"/>
    	<attribute name="contactNumber" mode="IN" type="String" optional="false"/>
    	<attribute name="email" mode="IN" type="String" optional="false"/>
    	<attribute name="partyId" mode="IN" type="String" optional="false"/>
    	<attribute name="toName" mode="IN" type="String" optional="true"/>
    	<attribute name="attnName" mode="IN" type="String" optional="true"/>
    	<attribute name="postalCode" mode="IN" type="String" optional="true"/>
    	
    	<attribute name="resultMap" mode="OUT" type="java.util.HashMap" optional="false"/>
    </service>
    
    <service name="findContactsInfo" engine="java"
            location="com.banfftech.personerp.PersonErpQueryService" invoke="findContactsInfo" auth="false">
        <description>查询用户信息</description>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service> 
    <service name="addContacts" engine="java"
            location="com.banfftech.personerp.PersonErpService" invoke="addContacts" auth="false">
        <description>添加联系人</description>
        <attribute name="personName" type="String" mode="IN" optional="false"/>
        <attribute name="gender" type="String" mode="IN" optional="false"/>
        <attribute name="contactNumber" type="String" mode="IN" optional="false"/>
        <attribute name="contactAddress1" type="String" mode="IN" optional="false"/>
        <attribute name="contactCity" type="String" mode="IN" optional="false"/>
        <attribute name="contactPostalCode" type="String" mode="IN" optional="false"/>
        <attribute name="contactGeoName" type="String" mode="IN" optional="false"/>
        <attribute name="contactAddress2" type="String" mode="IN" optional="true"/>
        <attribute name="contactEmail" type="String" mode="IN" optional="true"/>
        <attribute name="contactGroup" type="String" mode="IN" optional="true"/>
        <attribute name="contactCompany" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <service name="updateContacts" engine="java"
            location="com.banfftech.personerp.PersonErpService" invoke="updateContacts" auth="false">
        <description>更新联系人</description>
        <attribute name="personName" type="String" mode="IN" optional="false"/>
        <attribute name="gender" type="String" mode="IN" optional="false"/>
        <attribute name="contactNumber" type="String" mode="IN" optional="false"/>
        <attribute name="contactAddress1" type="String" mode="IN" optional="false"/>
        <attribute name="contactCity" type="String" mode="IN" optional="false"/>
        <attribute name="contactPostalCode" type="String" mode="IN" optional="false"/>
        <attribute name="contactGeoName" type="String" mode="IN" optional="false"/>
        <attribute name="contactAddress2" type="String" mode="IN" optional="true"/>
        <attribute name="contactEmail" type="String" mode="IN" optional="true"/>
        <attribute name="contactGroup" type="String" mode="IN" optional="true"/>
        <attribute name="contactCompany" type="String" mode="IN" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <service name="deleteContacts" engine="java"
            location="com.banfftech.personerp.PersonErpService" invoke="deleteContacts" auth="false">
        <description>删除联系人</description>
        <attribute name="partyIdTo" type="String" mode="IN" optional="false"/>
        <attribute name="partyIdFrom" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service> 
    <service name="findLable" engine="java"
            location="com.banfftech.personerp.PersonErpQueryService" invoke="findLable" auth="false">
        <description>查询拥有的标签</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service> 
    <service name="findLablePerson" engine="java"
            location="com.banfftech.personerp.PersonErpQueryService" invoke="findLablePerson" auth="false">
        <description>获取标签内的人员</description>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service> 
    <service name="deleteLable" engine="java"
            location="com.banfftech.personerp.PersonErpService" invoke="deleteLable" auth="false">
        <description>删除标签</description>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service> 
    <service name="addLablePerson" engine="java"
            location="com.banfftech.personerp.PersonErpService" invoke="addLablePerson" auth="false">
        <description>删除标签</description>
        <attribute name="partyIdFrom" type="String" mode="IN" optional="false"/>
        <attribute name="partyIdTo" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service> 
    <service name="createLable" engine="java"
            location="com.banfftech.personerp.PersonErpService" invoke="createLable" auth="false">
        <description>新建标签</description>
        <attribute name="lableName" type="String" mode="IN" optional="false"/>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <!-- 查询我的活动-->
    <service name="findMyEvent" engine="java"
             location="com.banfftech.personerp.PersonErpQueryService" invoke="findMyEvent" auth="false">
        <description>获取我的活动列表</description>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="roleTypeId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 报名 -->
    <service name="translationActivity" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="translationActivity" auth="false">
        <description>获取我的活动列表</description>
        <attribute name="workEffortId" type="String" mode="IN" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 查询活动明细 -->
    <service name="findMyEventDetail" engine="java"
             location="com.banfftech.personerp.PersonErpQueryService" invoke="findMyEventDetail" auth="false">
        <description>获取我的活动明细</description>
        <attribute name="workEffortId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <!-- 查询活动项目-->
    <service name="findActivityProjects" engine="java"
             location="com.banfftech.personerp.PersonErpQueryService" invoke="findActivityProjects" auth="false">
        <description>获取我的活动项目</description>
        <attribute name="workEffortId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 创建投票标题和问题 -->
    <service name="createSurveyAndQuestions" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="createSurveyAndQuestions" auth="false">
        <description>创建新的投票、并且关联活动和投票项</description>

        <attribute name="workEffortId" type="String" mode="IN" optional="false"/><!-- 活动标示 -->

        <attribute name="surveyName" type="String" mode="IN" optional="false"/><!-- 投票标题 -->
        <!--<attribute name="description" type="String" mode="IN" optional="true"/>投票描述 -->
        <attribute name="isAnonymous" type="String" mode="IN" optional="true"/><!-- 允许匿名投票,默认'否'N -->
        <attribute name="allowMultiple" type="String" mode="IN" optional="true"/><!-- 允许重复投票,默认'是'Y -->
        <attribute name="allowUpdate" type="String" mode="IN" optional="true"/><!-- 允许投票被更新,默认'是'Y -->

        <!-- 投票项分类 默认Poll Questions 1002 -->
        <!-- surveyQuestionTypeId BOOLEAN-->
        <!--<attribute name="qsDescriptions" type="String" mode="IN" optional="true"/> 暂时不提供 投票项描述  -->
        <attribute name="questions" type="String" mode="IN" optional="true"/><!-- 投票项名称 -->
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 活动的投票列表 -->
    <service name="findActivityPollQuestionsTitle" engine="java"
             location="com.banfftech.personerp.PersonErpQueryService" invoke="findActivityPollQuestionsTitle" auth="false">
        <description>创建新的标签并且关联联系人</description>
        <attribute name="workEffortId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 投票项目列表 -->
    <service name="findActivityPollQuestions" engine="java"
             location="com.banfftech.personerp.PersonErpQueryService" invoke="findActivityPollQuestions" auth="false">
        <description>创建新的标签并且关联联系人</description>
        <attribute name="surveyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 创建支付-->

    <service name="createPartyPaymentInfo" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="createPartyPaymentInfo" auth="false">
        <description>创建支付</description>
        <attribute name="partyIdFrom" type="String" mode="IN" optional="false"/>
        <attribute name="invoiceApplied" type="String" mode="IN" optional="false"/>
        <attribute name="workEffortId" type="String" mode="IN" optional="false"/>
        <attribute name="amountApplied" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 活动的账务信息 -->
    <service name="findActivityPayment" engine="java"
             location="com.banfftech.personerp.PersonErpQueryService" invoke="findActivityPayment" auth="false">
        <description>账务信息列表</description>

        <attribute name="workEffortId" type="String" mode="IN" optional="false"/>

        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>





    <!-- 用户点击投票 -->
    <service name="doPollQuestion" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="doPollQuestion" auth="false">
        <description>当前用户点击了投票</description>
        <attribute name="surveyQuestionId" type="String" mode="IN" optional="false"/>
        <attribute name="surveyId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 创建新的标签并且关联联系人 -->
    <service name="lableAndRelationContact" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="lableAndRelationContact" auth="false">
        <description>创建新的标签并且关联联系人</description>
        <attribute name="groupId" type="String" mode="IN" optional="false"/>
        <attribute name="partyArray" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 给活动创建活动项 -->
    <service name="createActivityProject" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="createActivityProject" auth="false">
        <description>创建活动项</description>
        <attribute name="workEffortId" type="String" mode="IN" optional="false"/>
        <attribute name="workEffortName" type="String" mode="IN" optional="true"/>
        <attribute name="description" type="String" mode="IN" optional="true"/>
        <attribute name="locationDesc" type="String" mode="IN" optional="true"/>
        <attribute name="actualStartDate" type="String" mode="IN" optional="true"/>
        <attribute name="estimatedCompletionDate" type="String" mode="IN" optional="true"/>
        <attribute name="inputMap" type="java.util.HashMap" mode="OUT" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>


    <!-- 开始进行一个新的活动、子活动-->
    <service name="createNewEvent" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="createNewEvent" auth="false">
        <description>开始一个新的活动</description>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
        <attribute name="parentId" type="String" mode="IN" optional="true"/><!-- 父活动id -->
        <attribute name="workEffortName" type="String" mode="IN" optional="true"/>
        <attribute name="contactsList" type="String" mode="IN" optional="true"/><!-- 受邀群体 -->
        <attribute name="description" type="String" mode="IN" optional="true"/>
        <attribute name="locationDesc" type="String" mode="IN" optional="true"/>
        <attribute name="actualStartDate" type="String" mode="IN" optional="true"/>
        <attribute name="estimatedCompletionDate" type="String" mode="IN" optional="true"/>
        <attribute name="scopeEnumId" type="String" mode="IN" optional="true"/>
        <!--<attribute name="uploadFile" type="java.nio.ByteBuffer" mode="IN" optional="true"/>-->
        <!--<attribute name="uploadFile_fileName" type="String" mode="IN" optional="true"/>-->
        <!--<attribute name="uploadFile_contentType" type="String" mode="IN" optional="true"/>-->

        <attribute name="inputMap" type="java.util.HashMap" mode="OUT" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <!-- app 登录 -->
    <service name="userAppLogin" engine="java"
             location="main.java.com.banfftech.personerp.PersonEventLoginWorker" invoke="userAppLogin" auth="false">
        <description>用户手机登录</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="captcha" type="String" mode="IN" optional="false"/>
        <attribute name="tarjeta" type="String" mode="OUT" optional="true"/>
        <!--<attribute name="organizationPartyId" type="String" mode="OUT" optional="true"/>-->
        <attribute name="inputMap" type="java.util.HashMap" mode="OUT" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <!-- 检查用户是否存在 -->
    <service name="isUserLoginExsits" engine="java"
             location="main.java.com.banfftech.personerp.PersonEventLoginWorker" invoke="isUserLoginExsits" auth="false">
        <description>用户手机登录</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>

        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>


    <!-- 组织者接受录入了一笔活动付款 -->
    <service name="createInvoiceAndPayment" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="createInvoiceAndPayment" auth="false">
        <description>创建活动项</description>
        <attribute name="partyIdTo" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <!-- 获得登录的验证码 -->
    <service name="getLoginCaptcha" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="getLoginCaptcha" auth="false">
        <description>获取登录验证码</description>
        <attribute name="teleNumber" type="String" mode="IN" optional="false"/>
        <attribute name="smsType" type="String" mode="IN" optional="false"/>
    </service>


    <!-- App用户注册 -->
    <service name="userAppRegister" engine="java"
             location="com.banfftech.personerp.PersonErpService" invoke="userAppRegister" auth="false">
        <description>appRegister</description>
        <attribute name="teleNumber" type="String" mode="IN" optional="false"/>
        <attribute name="nickName" type="String" mode="IN" optional="false"/>
        <attribute name="captcha" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>


</services>